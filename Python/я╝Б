import random
A = 0
while A < 4:
	computer = random.randint(1,3)
	my =int( input("1.石头\n 2.剪刀 \n3.布"))
	if my > 0 and my < 4:
		if my == 1:
			if my < computer:
				print("恭喜你赢了")
		if my == 2:
			if my < computer:
				print("恭喜你赢了")
		if my == 3:
			if my > computer:
				print("恭喜你赢了``")
	elif my == computer:
		print("平局")
	else:
		print("你输了")
	A+=1
